#include<bits/stdc++.h>

using namespace std;

int main(){
    int tt;
    cin >> tt;
    while(tt--){
        string word;
        char ch;
        cin >> word;
        cin >> ch;
        int len = word.length();
        if(word[len/2 + 1] == ch){
            cout << word[len/2 + 1] << endl;
            cout << ch << endl;
            cout << "YES" << "\n";
        }
        else{
            cout << "NO" << "\n";
        }
    }

    return 0;
}